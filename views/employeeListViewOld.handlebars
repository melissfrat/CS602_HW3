<div>
	<div class="table-responsive">
		<table class="table table-condensed">
			<thead>
				<tr>
					<th>First Name</th>
					<th>Last Name</th>
					<th></th>
				</tr>
			</thead>
			<tbody>
				{{#each data}}
					<tr>
						<td>{{this.firstName}}</td>
						<td>{{this.lastName}}</td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>




